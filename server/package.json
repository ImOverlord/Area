    "description": "Booster-TS Project",
    "main": "dist/app.js",
    "scripts": {
        "build": "tsc",
        "start": "node -r tsconfig-paths/register .",
        "serve": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/app.ts",
        "test": "echo \"Error: no test specified\" && exit 1",
