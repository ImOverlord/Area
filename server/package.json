        "express": "^4.17.1",
        "firebase-admin": "^8.9.2",
        "node-cron": "^2.0.3",
        "tsconfig-paths": "^3.9.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.2",
        "@types/jest": "^25.1.2",
        "@types/node-cron": "^2.0.2",
        "@types/superagent": "^4.1.7",
        "@typescript-eslint/eslint-plugin": "^2.19.0",
        "@typescript-eslint/parser": "^2.19.0",
        "eslint": "^6.8.0",
        "jest": "^25.1.0",
        "nodemon": "^2.0.2",
        "superagent": "^5.2.1",
        "ts-jest": "^25.2.0",
        "ts-node": "^8.6.2",
        "typescript": "^3.7.3"
    },
    "keywords": [],
    "author": "Booster-TS",
    "booster": {
